@page "/fetchdata"
@using BlazorApp_test
@using DataAccess.Repos.Todo;
@using DataAccess.Models;
@inject TodoRepository todoRepository

<PageTitle>Weather forecast</PageTitle>

<h1>Weather forecast</h1>

<p>This component demonstrates fetching data from a service.</p>


<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Temp. (C)</th>
            <th>Temp. (F)</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>

    </tbody>
</table>

@code {
    private IEnumerable<DataAccess.Models.Todo>? todos;

    protected async Task Load()
    {
        todos = await todoRepository.GetAll();
    }


}
